extends layout

block content
  h1= title
  div(class="form-inline")
    label(for="inputAddress", class="my-1 mr-2") Address

    div(class="custom-control custom-checkbox my-1 mr-sm-2")
      input(type="checkbox", class="custom-control-input", checked, id="expandAllCheck")
      label(class="custom-control-label", for="expandAllCheck") Exapnd All Packages

    div(class="input-group mr-sm-2 col-sm-4 col-md-5")
      input(type="text", class="form-control", id="inputAddress", placeholder="OAZUEUIFHISXGUFCBBRTJBRLIJJEJFFEIVSFHPNQRRHIXXKUCXVQNDIVXVNOICUWLLYEZVADHHULIEOFY")
      div(class="input-group-append")
        button(class="btn btn-outline-secondary", type="button", id="searchBtn")
          i(class="fas fa-search")

  div(class="row")
    div(class="col-sm-12")
      svg(width="100%", height="600", id="mainGraphSvg")