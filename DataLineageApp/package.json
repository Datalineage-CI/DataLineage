{
  "name": "data-lineage-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/server-app",
    "build": "cd ./src/server && tsc && cd ../cmds && tsc && cd .. && cd .. && webpack --mode=development",
    "build:prod": "cd ./src/server && tsc && cd ../cmds && tsc && cd .. && cd .. && webpack --mode=production",
    "build:cmds": "cd ./src/cmds && tsc",
    "clean:js": "del-cli dist/** && del-cli cmd_tools/**",
    "cmd:simulate": "node ./cmd_tools/cmds/mam-cmd.js -simulate"
  },
  "description": "DataLineageApp",
  "author": {
    "name": "DNVGL"
  },
  "devDependencies": {
    "@types/node": "8.10.12",
    "copy-webpack-plugin": "^4.5.1",
    "del-cli": "^1.1.0",
    "ts-loader": "^4.3.0",
    "ts-node": "^6.0.3",
    "typescript": "^2.8.3",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3"
  },
  "dependencies": {
    "@types/bootstrap": "^4.1.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/d3": "^5.0.0",
    "@types/debug": "^0.0.30",
    "@types/exit-hook": "^1.1.0",
    "@types/express": "^4.11.1",
    "@types/iota.lib.js": "^0.4.0",
    "@types/jquery": "^3.3.1",
    "@types/morgan": "^1.7.35",
    "body-parser": "^1.15.0",
    "cookie-parser": "^1.4.0",
    "debug": "^2.2.0",
    "exit-hook": "^2.0.0",
    "express": "^4.14.0",
    "flat-cache": "^1.3.0",
    "global-tunnel": "^1.2.0",
    "iota.lib.js": "^0.4.7",
    "morgan": "^1.7.0",
    "node-cache": "^4.2.0",
    "pug": "^2.0.0-beta6",
    "readline": "^1.3.0",
    "serve-favicon": "^2.3.0"
  },
  "-vs-binding": {
    "BeforeBuild": [
      "build"
    ]
  }
}
